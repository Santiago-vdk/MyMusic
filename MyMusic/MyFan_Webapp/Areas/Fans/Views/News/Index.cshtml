@model MyFan_Webapp.Areas.Fans.Models.FanProfileViewModel

@{
    Layout = "~/Areas/Fans/Views/Shared/_FanInBandProfileLayout.cshtml";
}
<!DOCTYPE html>

<head>
    <title>MyFan</title>
</head>


<body>
    <!-- Post-Section -->
    <div id="post-section" class="row">
        <h2 class="post-header">Looking at New, @Model.BandProfile.New.Id</h2>
        <div class="row">
            <div class="col-md-8">

                <form id="form-new-new" class='form-horizontal'>
                    <div class="form-group">
                        <div class="col-xs-3 text-right">
                            <label for="inputAlbumName">Title:</label>
                        </div>
                        <div class="col-xs-9">
                            <input type="text" class="form-control" placeholder="New Title" value="@Model.BandProfile.New.Title" name="NewTitle" id="NewTitle" disabled />
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-xs-3 text-right">
                            <label for="inputContent">Article:</label>
                        </div>
                        <div class="col-xs-9">
                            <div class="form-group">
                                <textarea class="form-control" rows="10" name="NewContent" placeholder="Article" id="inputContent" disabled>@Model.BandProfile.New.Content</textarea>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
</body>

